<?xml version="1.0" encoding="UTF-8"?>

<decorators defaultdir="/WebUI/decorators">
	<!-- excludes start -->
	<excludes>
		<pattern>/authorize.*</pattern>
		<pattern>/authorize/test.*</pattern>
		<pattern>/common/download.*</pattern>
		<pattern>/gpki*</pattern>
		<pattern>/notice/popup.*</pattern>
		<pattern>/data/file/innorixSendResult.*</pattern>
	</excludes>
	<!-- excludes end -->

	<!-- default layout start -->
	<decorator name="defaultLayout" page="defaultLayout.jsp">
		<pattern>/*</pattern>
	</decorator>
	<!-- popup -->
	<decorator name="popupLayout" page="popupLayout.jsp">
		<pattern>/hr/dept/findDeptPop.*</pattern>
		<pattern>/policy/loginPolicy/loginOptSettingPopup.*</pattern>
		<pattern>/policy/approvalPolicy/approvalLinePopup.*</pattern>
		<pattern>/policy/approvalPolicy/approvalLineUserPopup.*</pattern>
		<pattern>/policy/ipObjectPolicy/ipObjectPop.*</pattern>
		<pattern>/policy/ipObjectPolicy/ipObjectGroupPop.*</pattern>
		<pattern>/policy/destObjectPolicy/destObjectPop.*</pattern>
		<pattern>/policy/destObjectPolicy/destObjectGroupPop.*</pattern>
		<pattern>/policy/extPolicy/checkExtPopup.*</pattern>
		<pattern>/policy/extPolicy/checkMimeType.*</pattern>
		<pattern>/mail/sender/mailSendView.*</pattern>
	</decorator>
	<!-- default layout end -->

	<!-- index layout start -->
	<decorator name="newindex" page="indexNewLayout.jsp">
		<pattern>*/sign/index.*</pattern>
		<pattern>/sign/index_insu.*</pattern>
	</decorator>

	<!-- index layout start -->
	<decorator name="kinsIndex" page="indexKinsLayout.jsp">
		<pattern>/sign/index_kins.*</pattern>
	</decorator>

	<!-- index layout start -->
	<decorator name="koregIndex" page="indexKoregLayout.jsp">
		<pattern>/sign/index_koreg.*</pattern>
	</decorator>

	<!-- index layout start -->
	<decorator name="beforeIndex" page="indexBeforeLayout.jsp">
		<pattern>/sign/index_before.*</pattern>
	</decorator>
	
	<!-- index layout start -->
	<decorator name="ftcIndex" page="indexFtcLayout.jsp">
		<pattern>/site/ftc/newPortlnetCountAfter.*</pattern>
	</decorator>


	<!-- index layout start -->
	<decorator name="index" page="indexLayout.jsp">
		<pattern>/index.*</pattern>
		<pattern>/sign/stream.*</pattern>
		<pattern>/sign/trans.*</pattern>
		<pattern>/sign/sessionExpire.*</pattern>
		<pattern>/hr/user/changePasswordForm.*</pattern>
		<pattern>/hr/user/authOtpForm.*</pattern>
		<pattern>/common/defaultErrorMessage.*</pattern>
	</decorator>
	<!-- index layout end -->
	
	<!-- gpki index layout start -->
	<decorator name="index-gpki" page="/gpki/trs/index_decorator_gpki.jsp">
		<pattern>/sign/gpki/index.lin</pattern>
	</decorator>
	<!-- gpki index layout start -->

	<!-- index layout start -->
	<decorator name="init" page="initLayout.jsp">
		<pattern>/person/initPwd.*</pattern>
		<pattern>/person/changePassword.*</pattern>
	</decorator>
	<!-- index layout end -->

	<!-- expire layout start -->
	<decorator name="sessionExpire" page="expireLayout.jsp">
		<pattern>/sign/sessionDuplicate.*</pattern>
	</decorator>
	<!-- expire layout end -->
	
	<!-- trsMonitor layout start -->
	<decorator name="defaultTrsMonitor" page="defaultTrsMonitor.jsp">
		<pattern>/trsMonitor/*</pattern>
	</decorator>
	<!-- trsMonitor layout end -->

	<decorator name="panel" page="panel.jsp">
		<pattern>/patterntest.*</pattern>
	</decorator>
	<decorator name="printable" page="printable.jsp" />
	<decorator name="blank" page="blank.jsp" />
</decorators>
